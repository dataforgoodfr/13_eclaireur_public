{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install": "next build",
    "dev": "next dev --turbopack",
    "dev:scan": "NEXT_PUBLIC_REACT_SCAN=true next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check": "yarn lint && yarn typecheck",
    "format": "prettier --write \"(components|app|utils)/**/*.+(js|jsx|ts|tsx|json|css|md)\" --config ./prettier.config.js",
    "format:staged": "prettier --config ./prettier.config.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "storybook": "NEXT_PUBLIC_API_URL=http://localhost:6006 storybook dev -p 6006",
    "storybook:build": "NEXT_PUBLIC_API_URL=http://localhost:6006 storybook build",
    "backstop:test": "tsx scripts/visual-regression.ts test",
    "backstop:reference": "tsx scripts/visual-regression.ts reference",
    "backstop:approve": "backstop approve --config=backstop.json",
    "backstop:report": "backstop openReport --config=backstop.json",
    "backstop:interactive": "BACKSTOP_REMOTE_HTTP_PORT=3000  backstop remote --config=backstop.json",
    "lighthouse": "bun run lighthouse:mobile & bun run lighthouse:tablet & bun run lighthouse:desktop & wait",
    "lighthouse:mobile": "unlighthouse --site http://localhost:3000 --urls / /advanced-search?page=2 /map /community/213105554 /community/213105554/comparison/216901231?year=2023 --scanner.device mobile --lighthouseOptions.formFactor mobile --lighthouseOptions.screenEmulation.mobile true --lighthouseOptions.screenEmulation.width 393 --lighthouseOptions.screenEmulation.height 852 --lighthouseOptions.screenEmulation.deviceScaleFactor 3",
    "lighthouse:tablet": "unlighthouse --site http://localhost:3000 --urls / /advanced-search?page=2 /map /community/213105554 /community/213105554/comparison/216901231?year=2023 --scanner.device desktop --lighthouseOptions.formFactor desktop --lighthouseOptions.screenEmulation.mobile false --lighthouseOptions.screenEmulation.width 1024 --lighthouseOptions.screenEmulation.height 768 --lighthouseOptions.screenEmulation.deviceScaleFactor 1",
    "lighthouse:desktop": "unlighthouse --site http://localhost:3000 --urls / /advanced-search?page=2 /map /community/213105554 /community/213105554/comparison/216901231?year=2023 --scanner.device desktop --lighthouseOptions.formFactor desktop --lighthouseOptions.screenEmulation.mobile false --lighthouseOptions.screenEmulation.width 1440 --lighthouseOptions.screenEmulation.height 900 --lighthouseOptions.screenEmulation.deviceScaleFactor 1",
    "prepare": "cd .. && lefthook install",
    "scan:monitor": "REACT_SCAN=true REACT_SCAN_MONITOR=true next dev --turbopack",
    "scan:performance": "REACT_SCAN=true REACT_SCAN_PERFORMANCE=true next dev --turbopack",
    "scan:memory": "REACT_SCAN=true REACT_SCAN_MEMORY=true next dev --turbopack",
    "scan:export": "REACT_SCAN=true REACT_SCAN_EXPORT=true next dev --turbopack"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.66.4",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "fs": "^0.0.1-security",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.528.0",
    "maplibre-gl": "^5.2.0",
    "motion": "^12.23.12",
    "next": "^15.2.4",
    "nodemailer": "^6.10.0",
    "nuqs": "^2.4.3",
    "pg": "^8.13.3",
    "pg-copy-streams": "^6.0.6",
    "pg-promise": "^11.13.0",
    "playwright": "^1.54.1",
    "react": "^19.0.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-map-gl": "^8.0.1",
    "react-scan": "^0.4.3",
    "react-use-measure": "^2.1.7",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "topojson": "^3.0.2",
    "topojson-client": "^3.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "1.47.2",
    "@storybook/addon-a11y": "8",
    "@storybook/addon-essentials": "^8",
    "@storybook/addon-interactions": "^8",
    "@storybook/addon-links": "^8",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "^8",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/experimental-nextjs-vite": "^8.6.14",
    "@storybook/nextjs": "8",
    "@storybook/react": "8.6.14",
    "@storybook/test": "^8",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/d3": "^7.4.3",
    "@types/node": "^20",
    "@types/nodemailer": "^6",
    "@types/pg": "^8.11.11",
    "@types/pg-copy-streams": "^1.2.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "backstopjs": "^6.3.25",
    "cookie-signature-edge": "^1.0.7",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-storybook": "^9.0.18",
    "jsdom": "^26.1.0",
    "lefthook": "^1.12.2",
    "msw": "^2.10.4",
    "msw-storybook-addon": "^2.0.5",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "8",
    "storybook-addon-module-mock": "^1.4.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "imports": {
    "#utils/db": {
      "storybook": "./utils/__mocks__/db.ts",
      "default": "./utils/db.ts"
    },
    "#utils/fetchers/marches-publics/fetchMarchesPublics-server": {
      "storybook": "./utils/fetchers/marches-publics/__mocks__/fetchMarchesPublics-server.ts",
      "default": "./utils/fetchers/marches-publics/fetchMarchesPublics-server.ts"
    },
    "#utils/fetchers/marches-publics/fetchMarchesPublicsAvailableYears": {
      "storybook": "./utils/fetchers/marches-publics/__mocks__/fetchMarchesPublicsAvailableYears.ts",
      "default": "./utils/fetchers/marches-publics/fetchMarchesPublicsAvailableYears.ts"
    },
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
