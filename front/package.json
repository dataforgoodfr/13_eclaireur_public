{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install": "next build",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check": "yarn lint && yarn typecheck",
    "format": "prettier --write \"(components|app|utils)/**/*.+(js|jsx|ts|tsx|json|css|md)\" --config ./prettier.config.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "storybook": "NEXT_PUBLIC_API_URL=http://localhost:6006 storybook dev -p 6006",
    "storybook:build": "NEXT_PUBLIC_API_URL=http://localhost:6006 storybook build",
    "backstop:test": "tsx scripts/visual-regression.ts test",
    "backstop:reference": "tsx scripts/visual-regression.ts reference",
    "backstop:approve": "backstop approve --config=backstop.json",
    "backstop:report": "backstop openReport --config=backstop.json",
    "backstop:interactive": "BACKSTOP_REMOTE_HTTP_PORT=3000  backstop remote --config=backstop.json",
    "prepare": "cd .. && husky ./front/.husky"
  },
